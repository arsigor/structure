<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Login</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" media="screen" href="{{ asset('bundles/app/admin/css/bootstrap.min.css') }}" />
    <!-- Font Awesome -->
    <link rel="stylesheet" media="screen" href="{{ asset('bundles/app/admin/css/font-awesome.min.css') }}" />
    <!-- Ionicons -->
    <link rel="stylesheet" media="screen" href="{{ asset('bundles/app/admin/css/ionicons.min.css') }}" />
    <!-- Theme style -->
    <link rel="stylesheet" media="screen" href="{{ asset('bundles/app/admin/css/adminlte/AdminLTE.min.css') }}" />
    <!-- iCheck -->
    <link rel="stylesheet" media="screen" href="{{ asset('bundles/app/admin/css/blue.css') }}" />
    {% block style %}{% endblock %}
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="hold-transition login-page" style="height: 0">
{% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
    <div class="login-box">
        <div class="login-logo">
            <a href="{{ path('homepage') }}">Sciamanesimo</a>
        </div>
        <!-- /.login-logo -->
        <div class="login-box-body">
            <p class="login-box-msg">You are logged in</p>
            <div class="main-form">
                <div>
                    <a class="btn btn-warning btn-block btn-flat" href="{{ path('homepage') }}">
                        Go to profile
                    </a>
                </div>
            </div>
        </div>
        <!-- /.login-box-body -->
    </div>
    <!-- /.login-box -->
{% else %}
    {% block fos_user_content %}{% endblock fos_user_content %}
{% endif %}

<!-- jQuery 3 -->
<script src="{{ asset('bundles/app/admin/js/plugins/jQuery/jquery-2.2.3.min.js') }}"></script>
<!-- Bootstrap 3.3.7 -->
<script src="{{ asset('bundles/app/admin/js/bootstrap.min.js') }}"></script>
<!-- iCheck -->
<script src="{{ asset('bundles/app/admin/js/icheck.min.js') }}"></script>
<!-- DatePicker -->
<script src="{{ asset('bundles/app/admin/js/plugins/datepicker/bootstrap-datepicker.js') }}"></script>
<script src="{{ asset('bundles/app/admin/js/bootstrap-filestyle.js') }}"></script>
<script>
    $(function () {
        $('input').iCheck({
            checkboxClass: 'icheckbox_square-blue',
            radioClass: 'iradio_square-blue',
            increaseArea: '20%' // optional
        });
    });
</script>
{% block script %}{% endblock %}
</body>
</html>
